<launch>

  <arg name="uav_name" default="$(optenv UAV_NAME uav)"/>

  <group ns="$(arg uav_name)">

    <node name="mavros_interface" pkg="mrs_mavros_interface" type="mavros_interface" output="screen" >

      <!-- Subscribers --> 
      <remap from="~odometry_in" to="mavros/local_position/odom" />

      <!-- Publisher --> 
      <remap from="~odometry_out" to="~converted_mavros_odom" />

    </node>

  </group>

</launch>

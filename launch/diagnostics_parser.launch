<launch>

  <arg name="uav_name" default="$(optenv UAV_NAME uav)"/>

  <group ns="$(arg uav_name)">

    <node pkg="nodelet" type="nodelet" name="diagnostics_parser" args="load mrs_mavros_interface/DiagnosticsParser $(arg uav_name)_nodelet_manager" output="screen">

      <!-- Subscribers --> 
      <remap from="~diagnostics_in" to="/diagnostics" />
      <remap from="~mavros_state_in" to="mavros/state" />

      <!-- Publisher --> 
      <remap from="~diagnostics_out" to="mavros_interface/diagnostics" />
      <remap from="~profiler" to="profiler" />

    </node>

  </group>

</launch>
